<?xml version="1.0" encoding="utf-8"?>

<resources>

    <!-- 声明属性集的名称 -->
    <declare-styleable name="MyToggleButton">

        <!-- 声明一个属性 用于设置开关的状态 -->
        <attr name="slideBtnStat" format="boolean" />

        <!-- 设置背景图片 -->
        <attr name="bg" format="reference" />

        <!-- 设置按钮图片 -->
        <attr name="slideBtnbg" format="reference" />
    </declare-styleable>

</resources>